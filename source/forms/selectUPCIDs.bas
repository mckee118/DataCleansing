Public Sub ChangePTStatement(p_QueryName As String, p_sql As String)
'for changing pass-through's in this db
    Dim qdef As dao.QueryDef
 
    Set qdef = CurrentDb.QueryDefs(p_QueryName)
    qdef.SQL = p_sql
    qdef.Close
    Set qdef = Nothing
 
End Sub
